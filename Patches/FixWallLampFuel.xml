<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[@ParentName="WallLightBase"]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs[li="WoodLog"]</xpath>
        <value>
            <li>WoodLog</li>
            <li>WoodLog_Birch</li>
            <li>WoodLog_Cecropia</li>
            <li>WoodLog_Cypress</li>
            <li>WoodLog_Drago</li>
            <li>WoodLog_Maple</li>
            <li>WoodLog_Oak</li>
            <li>WoodLog_Palm</li>
            <li>WoodLog_Pine</li>
            <li>WoodLog_Poplar</li>
            <li>WoodLog_Teak</li>
            <li>WoodLog_Willow</li>
        </value>
    </Operation>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>VGP Xtra Trees and Flowers</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[@ParentName="WallLightBase"]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs[li="WoodLog"]</xpath>
            <value>
                <li>WoodLog_Acacia</li>
                <li>WoodLog_Camellia</li>
                <li>WoodLog_CherryBlossom</li>
                <li>WoodLog_JapaneseMaple</li>
                <li>WoodLog_RedMaple</li>
            </value>
        </match>
    </Operation>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Nature's Pretty Sweet</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>/Defs/ThingDef[@ParentName="WallLightBase"]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs[li="WoodLog"]</xpath>
            <value>
                <li>RedWoodLog</li>
            </value>
        </match>
    </Operation>
</Patch>
